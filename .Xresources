! Atom One Light theme
*background: #f9f9f9
*foreground: #383a42

*cursorColor: #d0d0d0
*color0: #000000
*color1: #E45649
*color2: #50A14F
*color3: #986801
*color4: #4078F2
*color5: #A626A4
*color6: #0184BC
*color7: #A0A1A7
*color8: #5c6370
*color9: #e06c75
*color10: #50A14F
*color11: #986801
*color12: #4078F2
*color13: #A626A4
*color14: #0184BC
*color15: #ffffff

Xft.autohint: 1
Xft.hintstyle: hindslight
Xft.lcdfilter: lcddefault
Xft.hinting: 1
Xft.antialias: 1

urxvt*geometry:         80x21
urxvt*font: xft:Hack-Regular:pixelsize=18
urxvt*boldFont: xft:Hack-Bold:pixelsize=18:weight=bold
URxvt.italicFont:   xft:Hack-RegularOblique:pixelsize=18:slant=italic

urxvt*scrollBar:        false
urxvt.urgentOnBell:     true

URxvt.perl-ext:
URxvt.perl-ext-common:
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

rofi.font: Pango 16
